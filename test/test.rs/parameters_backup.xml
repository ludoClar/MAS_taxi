<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
					<parameter name="randomSeed" displayName="Default Random Seed" type="int" 
						defaultValue="__NULL__"
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="satisfactionMin" displayName="Seuil de satisfaction minimal avant que le client s'en aille" type="int" 
						defaultValue="200" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="nbSources" displayName="Nombre de sources (minimum 2)" type="int" 
						defaultValue="100" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="satisfactionStep" displayName="Satisfaction Step" type="int" 
						defaultValue="200" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="pBabySeat" displayName="Pourcentage de taxis ayant des sieges bébés" type="int" 
						defaultValue="0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="pBaby" displayName="Pourcentage de clients ayant des bébés" type="int" 
						defaultValue="0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
		</parameters>


