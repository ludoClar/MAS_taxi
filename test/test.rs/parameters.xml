<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
					<parameter name="randomSeed" displayName="Default Random Seed" type="int" 
						defaultValue="__NULL__"
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="nbSources" displayName="Nombre de sources (minimum 2)" type="int" 
						defaultValue="100" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="nbTaxis" displayName="Nombre de taxis" type="int" 
						defaultValue="120" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
		<parameter name="satisfactionMin" displayName="Seuil de satisfaction minimal avant que le client parte" type="int" 
						defaultValue="200" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="satisfactionStep" displayName="Marge de satisfaction (Satisfaction max = satisfaction min + marge)" type="int" 
						defaultValue="300" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="pBabySeat" displayName="Pourcentage de taxis ayant des sieges bebes" type="int" 
						defaultValue="0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="pBaby" displayName="Pourcentage de clients ayant des bebes" type="int" 
						defaultValue="0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
		<parameter name="freqApparition" displayName="Nombre de ticks pour l'apparition des clients +/- 100 (105 minimal)" type="int" 
						defaultValue="250" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
		</parameters>


